@{
    ViewData["Title"] = "Index";
}
<div class="center">
    <blockquote class="text-center">@ViewBag.CurrentQuote</blockquote>
</div>
<div class="center">
    <input type="button" value="More Wisdom" onclick="location.href='@Url.Action("Index", "Kanye")'" />
</div>


<script>
    var typed = new Typed('#typed', {
        stringsElement: '#typed-strings'
    });
</script>
<div id="typed-strings">
    <p>Typed.js is a <strong>JavaScript</strong> library.</p>
    <p>It <em>types</em> out sentences.</p>
</div>
<span id="typed"></span>