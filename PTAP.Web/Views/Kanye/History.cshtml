@model IEnumerable<PTAP.Core.Models.KanyeQuote>

@{
    ViewData["Title"] = "History of Wisdom";
}

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QuoteText)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.QuoteText)
                </td>
            </tr>
        }
    </tbody>
</table>